#

# se_sv_dvorak as designed by tomas lundqvist. by moth

#FULL or OVERLAY
type FULL

# Don't know what HENKAN is, but I bind the extra
# apostrophe key on swedish keyboards to this.
# We'll use it for ; and :
map key 86 HENKAN

#map key 12 SLASH
#map key 53 MINUS
#map key 86 PLUS

### se sv dvorak keys ###

# for the */' key to the right of diaeresis a, rightmost key except enter
key BACKSLASH {
    label:                              ';'
    base:                               ';'
    shift:                              ':'
}

key A {
    label:                              'A'
    base:                               'a'
    shift, capslock:                    'A'
}

key B {
    label:                              'X'
    base:                               'x'
    shift, capslock:                    'X'
}

key C {
    label:                              'J'
    base:                               'j'
    shift, capslock:                    'J'
    alt:                                '\u00e7'
    shift+alt:                          '\u00c7'
}

key D {
    label:                              'E'
    base:                               'e'
    shift, capslock:                    'E'
}

key E {
    label:                              ','
    base:                               '.'
