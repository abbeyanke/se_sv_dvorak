}

key SPACE {
    label:                              ' '
    base:                               ' '
    alt, meta:                          fallback SEARCH
    ctrl:                               fallback LANGUAGE_SWITCH
}

key ENTER {
    label:                              '\n'
    base:                               '\n'
}

key TAB {
    label:                              '\t'
    base:                               '\t'
}

key COMMA {
    label:                              'W'
    base:                               'w'
    shift:                              'W'
}

key PERIOD {
    label:                              'V'
    base:                               'v'
    shift:                              'V'
}

key SLASH {
    label:                              'Z'
    base:                               'z'
    shift:                              'Z'
}

key GRAVE {
    label:                              '\''
    base:                               '\''
    shift:                              '~'
