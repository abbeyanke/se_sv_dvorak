    alt+shift:                          '\u0303'
}

key MINUS {
    label:                              '['
    base:                               '['
    shift:                              '{'
}

key EQUALS {
    label:                              ']'
    base:                               ']'
    shift:                              '}'
}

key LEFT_BRACKET {
    label:                              '['
    base:                               '/'
    shift:                              '?'
}

key RIGHT_BRACKET {
    label:                              '='
    base:                               '='
    shift:                              '+'
}

# custom bind in Generic.kl for swedish keymap on </> key
key HENKAN {
    label:				'\u00d6'
    base:				'\u00f6'
    shift, capslock:			'\u00d6'
}

key SEMICOLON {
    label:                              'S'
    base:                               's'
    shift, capslock:                    'S'
}

key APOSTROPHE {
    label:                              '-'
    base:                               '-'
    shift:                              '_'
}

### Numeric keypad ###

key NUMPAD_0 {
    label:                              '0'
    base:                               fallback INSERT
    numlock:                            '0'
}

key NUMPAD_1 {
    label:                              '1'
    base:                               fallback MOVE_END
    numlock:                            '1'
}

key NUMPAD_2 {
    label:                              '2'
